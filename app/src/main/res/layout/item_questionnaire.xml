<?xml version="1.0" encoding="utf-8"?>
<dong.lan.base.ui.customView.RelativeCarView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    app:rcv_elevation="4dp"
    app:rcv_elevationColor="@color/colorAccent"
    app:rcv_marginY="10dp"
    android:paddingTop="18dp"
    >

    <TextView
        android:id="@+id/item_questionnaire_intro"
        android:textSize="18sp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="12dp"
        android:layout_marginRight="60dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


    <TextView
        android:textColor="@color/colorPrimaryDark"
        android:id="@+id/item_questionnaire_res"
        android:layout_below="@id/item_questionnaire_intro"
        android:textSize="16sp"
        android:layout_margin="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />



    <dong.lan.library.LabelTextView
        app:bg_color="@color/colorPrimary"
        app:strokeWidth="1dp"
        android:text="新建"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        android:textColor="@color/colorPrimary"
        android:gravity="center"
        app:radius="12dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:id="@+id/item_questionnaire_status"
        android:layout_margin="4dp"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />



</dong.lan.base.ui.customView.RelativeCarView>